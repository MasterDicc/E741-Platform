pragma solidity ^0.8.24;


import '../interfaces/IE741V1Factory.sol';
import '../tokens/E741V1Token.sol';
import './E741V1FactoryBase.sol';

contract E741V1Factory741 is E741V1FactoryBase {

    constructor() {}

    function predict(bytes32 vanitySalt) public view override returns (address e741) {
        bytes memory bytecode = type(E741V1Token).creationCode;
        e741 = _predictWrapped(vanitySalt, bytecode);
    }

    function _predictWrapped(bytes32 vanitySalt, bytes memory bytecode) internal override view returns (address e741) {
        e741 = address(uint160(uint(keccak256(abi.encodePacked(
            bytes1(0xff),
            address(this),
            vanitySalt,
            keccak256(bytecode)
        )))));
    }

    function create(bytes32 _vanitySalt, bytes memory _encodedParameters) external override returns (address e741) {
        e741 = address(new E741V1Token{salt: _vanitySalt}());

        E741V1Token(e741).initialize(_encodedParameters);

        uint _allCollectionsLength = factoryStorage.pushNewCollection(e741, e741);

        // creator gets immediate ownership and totalsupply
        E741V1Token(e741).transfer(msg.sender, IERC20(e741).totalSupply());

        // change dev after transfering to prevent minting nfts
        E741V1Token(e741).changeDev(msg.sender);
        
        emit E741TokenCreated(msg.sender, e741, _allCollectionsLength);
    }

}