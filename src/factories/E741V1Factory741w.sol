import '../interfaces/IDevable.sol';
import '../interfaces/IE741V1Factory.sol';
import '../tokens/E741V1Wrapped741.sol';
import './E741V1FactoryBase.sol';

contract E741V1Factory741w is E741V1FactoryBase {

    constructor() {}

    function predict(bytes32 vanitySalt) public view override returns (address e741) {
        bytes memory bytecode = type(E741V1Wrapped741).creationCode;
        e741 = _predictWrapped(vanitySalt, bytecode);
    }

    function create(bytes32 _vanitySalt, bytes memory _encodedParameters) external override returns (address e741) {
        e741 = address(new E741V1Wrapped741{salt: _vanitySalt}());
        address _underlyingToken = IDevable(e741).initialize(_encodedParameters);
        uint _allCollectionsLength = factoryStorage.pushNewCollection(_underlyingToken, e741);
        IDevable(e741).changeDev(msg.sender); // give the caller dev
        emit E741Wrapped741Created(_underlyingToken, _vanitySalt, msg.sender, e741, _allCollectionsLength);
    }

