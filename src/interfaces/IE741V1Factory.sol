pragma solidity ^0.8.24;

import './IE741V1FactoryStorage.sol';


interface IE741V1Factory {
    event E741Wrapped20Created(address indexed token, bytes32 vanitySalt, address creator, address e741, uint allCollectionsLength);
    event E741Wrapped721Created(address indexed token, bytes32 vanitySalt, address creator, address e741, uint allCollectionsLength);
    event E741Wrapped741Created(address indexed token, bytes32 vanitySalt, address creator, address e741, uint allCollectionsLength);
    event E741TokenCreated(address creator, address e741, uint allCollectionsLength);

    function initialize(IE741V1FactoryStorage _factoryStorage) external;

    function getCollections(address token) external returns (address[] memory wrappers);
    function getCollection(address token, uint index) external returns (address wrapper);
    function collectionExists(address token) external view returns (bool exists);
    function allCollections(uint) external view returns (address pair);
    function allCollectionsLength() external view returns (uint);

    function predict( bytes32 vanitySalt) external view returns (address e741);
    function create(bytes32 vanitySalt, bytes memory encodedParameters) external returns (address e741);
}