pragma solidity ^0.8.24;


import '../interfaces/IDevable.sol';
import '../interfaces/IE741V1Factory.sol';
import '../interfaces/IE741V1FactoryStorage.sol';
import '../interfaces/IVanityDevable.sol';

interface IERC20Simple { // simplified
    function balanceOf(address account) external view returns (uint256);
    function transfer(address to, uint256 value) external returns (bool);
}

abstract contract E741V1FactoryBase is IE741V1Factory, IVanityDevable {

    IE741V1FactoryStorage public factoryStorage;

    mapping(address => bool) public dev;

    modifier onlyDev() { 
        require(dev[msg.sender], "Not the developer");
         _; 
    }

    constructor() {
        dev[msg.sender] = true; 
    }

    function initialize(IE741V1FactoryStorage _factoryStorage) external onlyDev {
        require(address(_factoryStorage) != address(0), "Need to set factory storage");
        factoryStorage = _factoryStorage;
    }

    receive() external payable {}

    function getCollections(address token) external override returns (address[] memory wrappers) {
        return factoryStorage.getCollections(token);
    }

    function getCollection(address token, uint index) external override returns (address wrapper) {
        return factoryStorage.getCollection(token, index);
    }

    function collectionExists(address token) external override view returns (bool exists) { 
        return factoryStorage.collectionExists(token);
    }

    function allCollections(uint index) external override view returns (address pair) {
        return factoryStorage.allCollections(index);
    }

    function allCollectionsLength() external override view returns (uint) {
        return factoryStorage.allCollectionsLength();
    }

    function _predictWrapped(bytes32 vanitySalt, bytes memory bytecode) internal virtual view returns (address e741) {
        e741 = address(uint160(uint(keccak256(abi.encodePacked(
            bytes1(0xff),
            address(this),
            vanitySalt,
            keccak256(bytecode)
        )))));
    }

    function updateDev(address newDev, bool enable) external onlyDev { 
        dev[newDev] = enable; 
    }

    function withdrawEth(address to) public onlyDev {
        (bool success,) = payable(to).call{value: address(this).balance}("");
        require(success);
    }

    function withdrawTokens(address to, address token) external onlyDev {
        IERC20Simple(token).transfer(to, IERC20Simple(token).balanceOf(address(this)));
    }

    // Forward calls to an IDevable wrapper we control
    // At the request of verified projects we may transfer control to the team
    function changeDev(address wrapper, address newDev) external onlyDev {
        IDevable(wrapper).changeDev(newDev);
    }

}