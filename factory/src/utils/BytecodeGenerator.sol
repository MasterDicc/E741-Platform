pragma solidity ^0.8.24;


contract Encoder {
    function v1Token(string memory _name, string memory _symbol, uint _decimals, string memory _baseURI, string memory _contractURI, uint _desiredTotalNft) external pure returns(bytes memory encodedPacked) {

        encodedPacked = abi.encode(_name, _symbol, _decimals ,_baseURI, _contractURI, _desiredTotalNft);

    }
    
    function v1Wrapped20(uint _fees, address _token, string memory _nameSuffix, string memory _symbolSuffix, uint _decimals, string memory _baseURI, string memory _contractURI, uint _desiredTotalNft, uint _underlyingWeiPerNft) external pure returns(bytes memory encodedPacked) {

        encodedPacked = abi.encode(_fees, _token, _nameSuffix, _symbolSuffix, _decimals, _baseURI, _contractURI, _desiredTotalNft, _underlyingWeiPerNft);

    }

    function v1Wrapped721(uint _fees, address _token, string memory _nameSuffix, string memory _symbolSuffix,  uint _decimals) external pure returns(bytes memory encodedPacked) {

        encodedPacked = abi.encode(_fees, _token, _nameSuffix, _symbolSuffix, _decimals);

    }

    function v1Wrapped741(uint _fees, address _token, string memory _name, string memory _symbol, uint _decimals, string memory _baseURI, string memory _contractURI, uint _desiredTotalNft, uint _underlyingWeiPerNft) external pure returns(bytes memory encodedPacked) {

        encodedPacked = abi.encode(_fees, _token, _name, _symbol, _decimals, _baseURI, _contractURI, _desiredTotalNft, _underlyingWeiPerNft);

    }
}